package test

// RecordList is a root type (unreferenced + exported)
type RecordList []Record

// Record is a helper type (referenced by RecordList)
type Record struct {
	Required     string  `json:"required"`
	OptionalStr  *string `json:"optionalStr"`
	OptionalInt  *int32  `json:"optionalInt"`
	OptionalBool *bool   `json:"optionalBool"`
}
